{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
       "target": "es5", // or higher, depending on your needs
    "module": "es2020", // or commonjs, depending on your needs
    "outDir": "dist", // output directory for compiled JavaScript files
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false



  }
}
